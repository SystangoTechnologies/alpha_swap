  "description": "",
  "dependencies": {
    "@cowprotocol/cow-sdk": "^7.1.1",
    "@cowprotocol/sdk-ethers-v6-adapter": "^0.2.0",
    "@google/generative-ai": "^0.24.1",
    "@types/node": "^24.10.1",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "ethers": "^6.15.0",
    "express": "^5.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5"
  }
